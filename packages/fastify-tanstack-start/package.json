{
  "name": "fastify-tanstack-start",
  "version": "0.1.0",
  "description": "Convenience plugin to serve Tanstack Start apps on a Fastify server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dev": {
      "types": "./dist/dev-plugin.d.ts",
      "import": "./dist/dev-plugin.js"
    },
    "./prod": {
      "types": "./dist/prod-plugin.d.ts",
      "import": "./dist/prod-plugin.js"
    }
  },
  "sideEffects": false,
  "files": ["dist"],
  "engines": {
    "node": ">=22.18"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "fastify",
    "production",
    "tanstack",
    "start",
    "tanstack-start",
    "server",
    "srvx",
    "ssr",
    "deployment",
    "vite"
  ],
  "author": "Wei Wang <wei@fastmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onlywei/fastify-tanstack-start.git"
  },
  "homepage": "https://github.com/onlywei/fastify-tanstack-start#readme",
  "bugs": {
    "url": "https://github.com/onlywei/fastify-tanstack-start/issues"
  },
  "dependencies": {
    "@fastify/middie": "^9.0.2",
    "@fastify/static": "^8.2.0",
    "fastify-plugin": "^5.1.0",
    "srvx": "^0.8.16"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/node": "catalog:",
    "fastify": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  },
  "peerDependencies": {
    "fastify": "catalog:",
    "vite": "catalog:"
  }
}
